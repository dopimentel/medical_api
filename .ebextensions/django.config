# DEBUGGING aTTeMPT
# This file is for debugging the container_commands execution environment.

option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: core.wsgi:application

container_commands:
  01_debug_venv_path:
    # Vamos listar o conteúdo do diretório do venv para ver o caminho real
    command: "ls -la /var/app/venv/ > /tmp/debug.log 2>&1"
  02_debug_python_path:
    # Vamos ver qual python está sendo usado
    command: "which python > /tmp/debug2.log 2>&1"